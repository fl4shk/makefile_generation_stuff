	SHARED_REGULAR_BUILD_PART_2 \
	HLL_REGULAR_BUILD_PART_3 \
	HLL_REGULAR_BUILD_PART_4(comp,flags_prefix) \
	HLL_REGULAR_BUILD_PART_5 \
	HLL_REGULAR_BUILD_PART_6 \
	HLL_REGULAR_BUILD_PART_7
