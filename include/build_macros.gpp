	HLL_ASMOUTS_BUILD_PART_2A(comp,flags_prefix) \
	HLL_ASMOUTS_BUILD_PART_2B(comp,flags_prefix) \
	HLL_ASMOUTS_BUILD_PART_2(comp,flags_prefix) \
	HLL_ASMOUTS_BUILD_PART_3 \
	HLL_ASMOUTS_BUILD_PART_4 \
	HLL_ASMOUTS_BUILD_PART_5
